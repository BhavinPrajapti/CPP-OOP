#include "Bid.h"

Bid::Bid(const std::shared_ptr<User> &user, double amount) : user(user) , amount(amount)
{
}

std::shared_ptr<User> Bid::getUser() const
{
	return this->user;
}

double Bid::getAmount() const
{
	return this->amount;
}
